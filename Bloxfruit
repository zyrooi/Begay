        blackScreen.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        blackScreen.Parent = game:GetService("CoreGui")
    else
        local blackScreen = game:GetService("CoreGui"):FindFirstChild("BlackScreen")
        if blackScreen then
            blackScreen:Destroy()
        end
    end
end)
